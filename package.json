{
  "name": "eggsplash",
  "version": "1.2.5",
  "description": "Unsplash but only egg photos",
  "main": "index.js",
  "scripts": {
    "start": "node server/dist/index.js",
    "dev": "nodemon",
    "dev:client": "parcel client/src/index.html",
    "build": "npm run build:client && npm run build:server",
    "build:server": "tsc",
    "build:client": "parcel build client/src/index.html --out-dir client/dist/ --public-url ./ --no-source-maps"
  },
  "browserslist": [
    "last 20 Chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patricklatorre/eggsplash.git"
  },
  "author": "Patrick Latorre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patricklatorre/eggsplash/issues"
  },
  "homepage": "https://github.com/patricklatorre/eggsplash#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^2.6.0",
    "unsplash-js": "^6.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jquery": "^3.3.33",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node-fetch": "^2.5.4",
    "@types/unsplash-js": "^6.0.1",
    "cssnano": "^4.1.10",
    "nodemon": "^2.0.2",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.25.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}